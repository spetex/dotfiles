// Place your key bindings in this file to overwrite the defaults
[
  {
  "key": "ctrl+k ctrl+0",
  "command": "editor.foldAll",
  "when": "editorTextFocus"
  },
  {
  "key": "ctrl+k ctrl+9",
  "command": "editor.unfoldAll",
  "when": "editorTextFocus"
  },
  {
    "key": "ctrl+o",
    "command": "workbench.action.files.openFolder"
  },
  {
    "key": "ctrl+'",
    "command": "extension.swapQuotes",
    "when": "editorTextFocus"
  }
  // Diable arrow keys
  // { "key": "up",                    "command": ""},
  // { "key": "down",                    "command": ""},
  // { "key": "left",                    "command": ""},
  // { "key": "right",                    "command": ""}
]